<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="pos_technotrade_view_partner_form" model="ir.ui.view">
            <field name="name">pos_technotrade.res.partner.form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <notebook position="inside">
                    <page string="Matriculas" name="plate_numbers">
                        <group>
                            <field name="plate_number_ids" widget="section_and_note_one2many" mode="tree">
                             </field>   
                        </group>    
                    </page>    
                </notebook>    
             </field>
        </record>
    </data>    

</odoo>
